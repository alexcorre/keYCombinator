{
  "manifest_version": 2,
  
  "name": "keYCombinator",
  "description": "Keyboard Shortcuts for Getting Around Hacker News",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["https://news.ycombinator.com/*"],
      "css": ["css/main.css"],
      "js": ["js/jquery-2.0.3.min.js", "js/main.js"]
    }
  ]
}
